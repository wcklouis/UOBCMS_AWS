    <div id="BankAccount" class="tabcontent" style="display: none;">
        <table class="formcontractterm" id="tbBankAccList" border="1" cellspacing=0 cellpadding=7>
            <thead>
                <tr>
                    <td colspan="10"><div><h4><u>HKD Bank Account list</h4></u></div></td>
                </tr>
            <th>Bank Code</th>
            <th>Bank Name</th>
            <th>Bank Account Name</th>
            <th>Bank Ccy</th>
            <th>Bank Account Number</th>
            <th>Account Number</th>
            <th>Payment Type</th>
            <th>FPS Payment Type</th>
            <th>Default HKD Bank</th>
            <th>Status</th>
            </thead>
            <tbody>
                @foreach (var client_account in Model.Cms_client_accounts)
                {
                    // Check if Cms_account is not null
                    if (client_account.Cms_account != null)
                    {
                        foreach (var account_bank in client_account.Cms_account.Cms_account_banks)
                        {
                            // Check if Cms_client_bank and its Cms_bank are not null
                            if (account_bank.Cms_client_bank != null && account_bank.Cms_client_bank.Cms_bank != null && account_bank.Cms_client_bank.Status != "V")
                            {
                <tr>
                    <td>@account_bank.Cms_client_bank.Cms_bank.Bank_code</td>
                    <td>@account_bank.Cms_client_bank.Cms_bank.Bank_name</td>
                    <td>@account_bank.Cms_client_bank.Bank_accname</td>
                    <td>@account_bank.Cms_client_bank.Ccy</td>
                    <td>@account_bank.Cms_client_bank.Bank_accno</td>
                    <td>@account_bank.Cms_account.AccNo</td>
                    <td>@account_bank.Cms_client_bank.Default_payment_type_string</td>
                    <td>@account_bank.Cms_client_bank.Fps_payment_type_string</td>
                    <td>@account_bank.DefaultLocalHKDBankString</td>
                    <td>@account_bank.Cms_client_bank.StatusString</td>
                </tr>
                            }
                        }
