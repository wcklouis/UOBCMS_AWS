                    <td>@account_bank.Cms_client_bank.Corr_bank_code</td>
                    <td>@account_bank.Cms_client_bank.Corr_bank_swift</td>
                    <td>@account_bank.Cms_client_bank.Msg_to_bank</td>
                    <td>@account_bank.Cms_account.AccNo</td>
                    <td>@account_bank.Cms_client_bank.Default_payment_type_string</td>
                    <td>@account_bank.Cms_client_bank.StatusString</td>
                </tr>
                            }
                        }
                    }
                }
            </tbody>
        </table>
    </div>
    <br /><br />

    <div id="Phone" class="tabcontent" style="display: none;">
        <table class="formcontractterm" id="tbPhoneList" border="1" cellspacing=0 cellpadding=7>
            <thead>
                <tr>
                    <td colspan="6"><div><h4><u>Phone list</h4></u></div></td>
                </tr>
            <th>Type</th>
            <th>SubType</th>
            <th>Country Code</th>
            <th>Area Code</th>
            <th>Phone Number</th>
            <th>Account Number</th>
            </thead>
            <tbody>

                @{
                // Create a list to hold all phones
                var allPhones = new List<(string Type, string TypeString, string SubTypeString, string Country_code, string Area_code, string Value, string AccNo)>();

                // Collect all phone data
                foreach (var client_account in Model.Cms_client_accounts)
                {
                    if (client_account.Cms_account != null)
                    {
                        foreach (var account_phone in client_account.Cms_account.Cms_account_phones)
                        {
                            allPhones.Add((
                                account_phone.Cms_client_phone.Type,
                                account_phone.Cms_client_phone.TypeString,
                                account_phone.Cms_client_phone.SubTypeString,
                                account_phone.Cms_client_phone.Country_code,
                                account_phone.Cms_client_phone.Area_code,
                                account_phone.Cms_client_phone.Value,
                                account_phone.Cms_account.AccNo
                            ));
                        }
                    }
                }

                // Sort the phones by type
                var sortedPhones = allPhones.OrderBy(p => p.Type).ToList();

                // Render sorted phone data
                foreach (var phone in sortedPhones)
                {
                    <tr>
                        <td>@phone.TypeString</td>
                        <td>@phone.SubTypeString</td>
                        <td>@phone.Country_code</td>
                        <td>@phone.Area_code</td>
                        <td>@phone.Value</td>
                        <td>@phone.AccNo</td>
                    </tr>
                }
                }
            </tbody>
        </table>
    </div>
    <br /><br />

    <div id="Email" class="tabcontent" style="display: none;">
