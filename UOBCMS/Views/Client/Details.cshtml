                    // Check if Cms_account is not null
                    if (client_account.Cms_account != null)
                    {
                        foreach (var account_email in client_account.Cms_account.Cms_account_emails)
                        {
                <tr>
                    <td>@account_email.Cms_client_email.TypeString</td>
                    <td>@account_email.Cms_client_email.Email</td>
                    <td>@account_email.Cms_account.AccNo</td>
                </tr>
